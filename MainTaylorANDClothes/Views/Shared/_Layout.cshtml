<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Boostrap 4/css/boostrap.min.css" rel="stylesheet" />
    <link href="~/Content/Boostrap 4/css/data-table.min.css" rel="stylesheet" />

</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container">
            <a class="navbar-brand" href="#">Main&Taylor</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <script src="~/Content/Boostrap 4/js/jquery.min.js"></script>
    <script src="~/Content/Boostrap 4/js/poper.min.js"></script>
    <script src="~/Content/Boostrap 4/js/bootstrap.min.js"></script>
    <script src="~/Content/Boostrap 4/js/data-table.min.js"></script>
    @RenderSection("myscript", required: false)
</body>
</html>
